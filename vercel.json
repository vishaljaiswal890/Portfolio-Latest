{
    "version": 2,
    "builds" : [
        { "src": "./Frontend/build/", "use": "@vercel/static" },
        { "src": "*.py", "use": "@vercel/python" },
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src" : "/server.js/*",
            "dest": "/"
        }
    ]
}