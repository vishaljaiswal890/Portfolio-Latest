{
    "version": 2,
    "builds" : [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        },
        {
            "src": "./Frontend/build/index.html",
            "use": "@vercel/static"
        }

    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "./server.js"
        }
    ]
}